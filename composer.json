{
    "name": "apploud/translations-converter",
    "description": "Simple extension for Nette framework that converts translations from multiple lang files in Neon syntax to one Excel file (xlsx) and vice versa.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Jan Konáš",
            "email": "jan.konas@apploud.cz"
        }
    ],
    "autoload": {
        "psr-4": {
            "Apploud\\TranslationsConverter\\": "src"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "nette/di": "^2.2",
        "nette/finder": "^2.2",
        "nette/utils": "^2.2",
        "phpoffice/phpexcel": "^1.8",
        "symfony/console": "^4.1"
    },
    "require-dev": {
        "nette/tester": "^1.7",
        "nette/bootstrap": "^2.2",
        "nette/robot-loader": "^2.2",
        "nette/mail": "^2.2",
        "tracy/tracy": "^2.2",
        "nette/safe-stream": "^2.2"
    }
}
